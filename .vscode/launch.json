  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "WSOCKET",
      "program": "${workspaceFolder}/app/wsocket.ts",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "SOCKET",
      "program": "${workspaceFolder}/app/socket.ts",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CLUSTER",
      "program": "${workspaceFolder}/app/cluster.ts",
      "autoAttachChildProcesses": true,
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "APP",
      "program": "${workspaceFolder}/app/index.ts",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "outputCapture": "std"
    }
  ]
}